            transition: transform 0.3s ease, box-shadow 0.3s ease, background-color 0.3s ease;
            display: block;
            height: 100%;
            box-sizing: border-box;
        }

        .level a:hover {
            transform: translateY(-5px);
            box-shadow: 0 8px 16px rgba(0, 0, 0, 0.4);
            background-color: rgba(255, 255, 255, 0.1);
        }

