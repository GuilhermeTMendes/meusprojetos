        .level ul {
            list-style-type: none;
            padding: 0;
            margin: 0;
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
            gap: 10px;
        }

        .level a {
            background-color: rgba(255, 255, 255, 0.05);
            padding: 8px 12px;
            border-radius: 5px;
            text-align: center;
            font-size: 0.9em;
            color: inherit;
